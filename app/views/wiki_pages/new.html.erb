<h1><%= t('label.wiki_page.creating') %></h1>

<div id="preview_place"> </div>

<% form_for(@wiki_page, :url => wiki_pages_path) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form" , :locals => {:f => f}%>

  <p>
  <%= submit_to_remote("preview_btn", t('label.common.preview'),
       :url => {:action => "preview"}, :update => {:success => 'preview_place'}) %>
    <%= f.submit t('label.common.create') %>
  </p>
<% end %>

<%= link_to t('label.common.back'), wiki_pages_path %>
